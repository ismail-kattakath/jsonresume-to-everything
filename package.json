{
  "name": "jsonresume-to-everything",
  "version": "0.1.0",
  "description": "Transform one JSON Resume into a complete portfolio ecosystem: responsive website, interactive resume builder, AI-powered cover letter generator, and more. Built with Next.js 15, TypeScript, Tailwind CSS. Deploy to GitHub Pages in 10 minutes. Fully tested, customizable, and SEO-optimized.",
  "author": "Ismail Kattakath",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ismail-kattakath/jsonresume-to-everything"
  },
  "bugs": {
    "url": "https://github.com/ismail-kattakath/jsonresume-to-everything/issues"
  },
  "homepage": "https://ismail.kattakath.com",
  "keywords": [
    "jsonresume",
    "portfolio",
    "resume-builder",
    "cover-letter",
    "nextjs",
    "typescript",
    "tailwindcss",
    "github-pages",
    "ats-resume",
    "static-site"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:reload": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && rm -rf .next && npm run dev",
    "build": "next build",
    "download-resume": "if [ -f .env.local ]; then node --env-file=.env.local scripts/download-resume.mjs; else node scripts/download-resume.mjs; fi",
    "postinstall": "npm run download-resume",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint src",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "export": "next build && next export",
    "deploy": "npm run build && npx gh-pages -d out -t true",
    "deploy-auto": "nohup npm run deploy > /tmp/deploy.log 2>&1 &"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@lottiefiles/dotlottie-react": "^0.17.14",
    "@next/third-parties": "^16.0.5",
    "@strands-agents/sdk": "^0.2.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "gh-pages": "^6.3.0",
    "lucide-react": "^0.542.0",
    "next": "16.0.4",
    "onborda": "^1.2.5",
    "openai": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-highlight-menu": "^2.0.2",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.16.0",
    "react-tooltip": "^5.30.0",
    "react-typed": "^2.0.12",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-jsdoc": "^61.4.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4",
    "typescript": "5.9.3"
  }
}
