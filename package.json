{
  "name": "ai-jsonresume",
  "version": "0.1.0",
  "description": "Transform one JSON Resume into a complete portfolio ecosystem: responsive website, interactive resume builder, AI-powered cover letter generator, and more. Built with Next.js 15, TypeScript, Tailwind CSS. Deploy to GitHub Pages in 10 minutes. Fully tested, customizable, and SEO-optimized.",
  "author": "Ismail Kattakath",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ismail-kattakath/ai-jsonresume"
  },
  "bugs": {
    "url": "https://github.com/ismail-kattakath/ai-jsonresume/issues"
  },
  "homepage": "https://ismail.kattakath.com",
  "keywords": [
    "jsonresume",
    "portfolio",
    "resume-builder",
    "cover-letter",
    "nextjs",
    "typescript",
    "tailwindcss",
    "github-pages",
    "ats-resume",
    "static-site"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:reload": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && rm -rf .next && npm run dev",
    "build": "next build",
    "download-resume-json": "if [ -f .env.local ]; then node --env-file=.env.local scripts/download-resume-json.mjs; else node scripts/download-resume-json.mjs; fi",
    "sync-resume-markdown": "if [ -f .env.local ]; then npx tsx --env-file=.env.local scripts/sync-resume-markdown.ts; else npx tsx scripts/sync-resume-markdown.ts; fi",
    "postinstall": "npm run download-resume-json",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "export": "next build && next export",
    "deploy": "npm run build && npx gh-pages -d out -t true",
    "deploy-auto": "nohup npm run deploy > /tmp/deploy.log 2>&1 &",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/genai": "^1.41.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@lottiefiles/dotlottie-react": "^0.17.14",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@next/third-parties": "^16.0.5",
    "@radix-ui/react-portal": "^1.1.10",
    "@strands-agents/sdk": "^0.2.1",
    "@testing-library/dom": "^10.4.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.3",
    "lucide-react": "^0.575.0",
    "next": "^16.1.6",
    "onborda": "^1.2.5",
    "openai": "^6.22.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-highlight-menu": "^2.0.2",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.16.0",
    "react-tooltip": "^5.30.0",
    "react-typed": "^2.0.12",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.1.6",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.1",
    "baseline-browser-mapping": "^2.10.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-jsdoc": "^62.7.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-security": "^3.0.1",
    "gh-pages": "^6.3.0",
    "glob": "^13.0.6",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-cli": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rimraf": "^6.1.3",
    "tailwindcss": "^4.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.56.0"
  },
  "overrides": {
    "test-exclude": "^7.0.1",
    "glob": "^13.0.6",
    "rimraf": "^6.1.3",
    "minimatch": "^9.0.5",
    "tough-cookie": "^4.1.3",
    "qs": "^6.14.1",
    "micromatch": "^4.0.8",
    "brace-expansion": "^2.0.1",
    "kind-of": "^6.0.3",
    "node-notifier": "^10.0.1",
    "yargs-parser": "^21.1.1",
    "minimist": "^1.2.8",
    "cross-spawn": "^7.0.6"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "bash -c 'npx tsc --noEmit'",
      "npm test -- --findRelatedTests --passWithNoTests",
      "npm run format",
      "npm run lint:fix"
    ]
  }
}