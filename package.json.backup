{
  "name": "jsonresume-to-everything",
  "version": "0.1.0",
  "description": "Transform one JSON Resume into a complete portfolio ecosystem: responsive website, interactive resume builder, AI-powered cover letter generator, and more. Built with Next.js 15, TypeScript, Tailwind CSS. Deploy to GitHub Pages in 10 minutes. Fully tested, customizable, and SEO-optimized.",
  "author": "Ismail Kattakath",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ismail-kattakath/jsonresume-to-everything"
  },
  "bugs": {
    "url": "https://github.com/ismail-kattakath/jsonresume-to-everything/issues"
  },
  "homepage": "https://ismail.kattakath.com",
  "keywords": [
    "jsonresume",
    "portfolio",
    "resume-builder",
    "cover-letter",
    "nextjs",
    "typescript",
    "tailwindcss",
    "github-pages",
    "ats-resume",
    "static-site"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "export": "next build && next export",
    "deploy": "npm run build && npx gh-pages -d out -t true",
    "deploy-auto": "nohup npm run deploy > /tmp/deploy.log 2>&1 &",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "gh-pages": "^6.3.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-highlight-menu": "^2.0.2",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.16.0",
    "react-typed": "^2.0.12",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-jsdoc": "^61.4.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
